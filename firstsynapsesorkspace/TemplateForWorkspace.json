							"CREATE TABLE dbo.test2\r\n",
							"    (\r\n",
							"     [CustomerID] int,\r\n",
							"     [Product] varchar(100),\r\n",
							"     [Feedback] varchar(4000)\r\n",
							"     \r\n",
							"    )\r\n",
							"WITH\r\n",
							"    (\r\n",
							"    DISTRIBUTION = ROUND_ROBIN,\r\n",
							"     CLUSTERED COLUMNSTORE INDEX\r\n",
							"     -- HEAP\r\n",
							"    )\r\n",
							"GO\r\n",
							"\r\n",
							"COPY INTO dbo.test\r\n",
							"([CustomerID] ,[Product], [Feedback])\r\n",
							"FROM 'https://dlgstor.blob.core.windows.net/source/test/Customer Feedback.csv'\r\n",
