				"source": [
					"IF NOT EXISTS (SELECT * FROM sys.objects O JOIN sys.schemas S ON O.schema_id = S.schema_id WHERE O.NAME = 'test4' AND O.TYPE = 'U' AND S.NAME = 'dbo')\r\n",
					"CREATE TABLE dbo.test4\r\n",
					"    (\r\n",
					"     [CustomerID] int,\r\n",
					"     [Product] varchar(100),\r\n",
					"     [Feedback] varchar(4000)\r\n",
					"     \r\n",
					"    )\r\n",
					"WITH\r\n",
					"    (\r\n",
					"    DISTRIBUTION = ROUND_ROBIN,\r\n",
					"     CLUSTERED COLUMNSTORE INDEX\r\n",
					"     -- HEAP\r\n",
					"    )\r\n",
					"GO\r\n",
					"\r\n",
					"COPY INTO dbo.test4\r\n",
					"([CustomerID] ,[Product], [Feedback])\r\n",
					"FROM 'https://dlgstor.blob.core.windows.net/source/test/Customer Feedback.csv'\r\n",
					"WITH\r\n",
					"(\r\n",
					"    FILE_TYPE = 'CSV'\r\n",
					"         ,HEADER_ROW = 2\r\n",
					"    ,MAXERRORS = 0\r\n",
					"    ,IDENTITY_INSERT = 'OFF'\r\n",
					")"
				],
				"execution_count": 1
			}
		]
